<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xmlns:web="http://java.sun.com/xml/ns/javaee" 
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
  id="WebApp_ID" version="2.5">
  <display-name>Spring3MVC</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HelloWorldServlet</servlet-name>
    <servlet-class>
			net.viralpatel.maven.HelloWorldServlet
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorldServlet</servlet-name>
    <url-pattern>/hello-world</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DownloadHDFS</display-name>
    <servlet-name>DownloadHDFS</servlet-name>
    <servlet-class>com.passparam.download.DownloadHDFS</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownloadHDFS</servlet-name>
    <url-pattern>/DownloadHDFS</url-pattern>
  </servlet-mapping>
</web-app>